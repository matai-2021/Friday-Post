{
  "name": "friday-post",
  "version": "1.0.0",
  "description": "Friday project by POST (Eleanor, James, Keisuke and Westley)",
  "main": "server/index.js",
  "scripts": {
    "start": "run-p webpack server",
    "webpack": "webpack --config client/webpack.config.js",
    "dev": "run-p dev:client dev:server",
    "dev:client": "webpack --watch --config client/webpack.config.js",
    "dev:server": "nodemon --watch server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx ."
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matai-2021/Friday-Post.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matai-2021/Friday-Post/issues"
  },
  "homepage": "https://github.com/matai-2021/Friday-Post#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "7.9.0",
    "eslint-config-eda": "0.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "superagent": "^6.1.0",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@babel/register": "^7.12.10",
    "express": "^4.17.1",
    "knex": "^0.21.13",
    "react-router-dom": "^5.2.0",
    "sqlite3": "^5.0.0"
  }
}
